{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "packageRules": [
    {
      "groupName": "all non-major dependencies go",
      "groupSlug": "all-minor-patch-go",
      "matchPackageNames": [
        "golang",
        "go"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ]
}
